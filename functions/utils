# -*- mode: sh -*-

GO(show) {
    declare -f {{__}}GO_CMD{{:$1}}
}

GO(commands) {
    compgen -A function "{{__}}GO_CMD{{:}}" | cut -d: -f 2 | tr '\n' ' '
}

GO(link) {
  BLOCK({{
    if [ -d functions ]
    then
        TARGET="${__GO_DIR}/functions/${PWD##*/}"
        ln -sin ${PWD}/functions ${TARGET}
        set +x
        GO_CMD update
    fi
  }})
}
